{
    "builds": [{
        "src": "shoppinglyx/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "shoppinglyx/wsgi.py"
        }
    ],
    "scripts": {
    "build": "pip install -r requirements.txt"
  }

}
